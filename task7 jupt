   "cell_type": "code",
   "execution_count": 31,
   "id": "dd371e5b-ba47-4b35-b7a3-4b98b5b31d8f",
   "metadata": {},
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "    product  total_qty  revenue\n",
      "0  Keyboard        220  16890.0\n",
      "1    Laptop         60  73020.0\n",
      "2   Monitor         80  24000.0\n",
      "3     Mouse        600  14800.0\n",
      "4    Webcam         60   3300.0\n"
     ]
    }
   ],
   "source": [
    "import sqlite3\n",
    "import pandas as pd\n",
    "import matplotlib.pyplot as plt\n",
    "# Connect to SQLite database\n",
    "conn = sqlite3.connect(\"sales_data.db\")\n",
    "\n",
    "# Run SQL query to get total quantity & revenue per product\n",
    "query = \"\"\"\n",
    "    SELECT product, SUM(quantity) AS total_qty, SUM(quantity * price) AS revenue \n",
    "    FROM sales \n",
    "    GROUP BY product\n",
    "\"\"\"\n",
    "df = pd.read_sql_query(query, conn)  # Load query results into DataFrame\n",
    "conn.close()  # Close database connection\n",
    "\n",
    "# Print results\n",
    "print(df)\n",
    "\n"
   ]
  },
