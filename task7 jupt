   "source": [
    "import sqlite3\n",
    "\n",
    "# Connect to SQLite database\n",
    "conn = sqlite3.connect(\"sales_data.db\")\n",
    "cursor = conn.cursor()\n",
    "\n",
    "# Create the sales table\n",
    "cursor.execute(\"\"\"\n",
    "    CREATE TABLE IF NOT EXISTS sales (\n",
    "        sale_id INTEGER PRIMARY KEY AUTOINCREMENT,\n",
    "        product TEXT,\n",
    "        quantity INTEGER,\n",
    "        price REAL\n",
    "    )\n",
    "\"\"\")\n",
    "\n",
    "# Insert sample data\n",
    "sales_data = [\n",
    "    (\"Laptop\", 10, 1200.50),\n",
    "    (\"Mouse\", 50, 25.00),\n",
    "    (\"Keyboard\", 30, 75.75),\n",
    "    (\"Monitor\", 20, 300.00),\n",
    "    (\"Laptop\", 5, 1250.00),\n",
    "    (\"Mouse\", 100, 24.50),\n",
    "    (\"Webcam\", 15, 55.00),\n",
    "    (\"Keyboard\", 25, 78.00)\n",
    "]\n",
    "\n",
    "cursor.executemany(\"INSERT INTO sales (product, quantity, price) VALUES (?, ?, ?)\", sales_data)\n",
    "conn.commit()\n",
    "conn.close()\n"
   ]
